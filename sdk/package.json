{
  "name": "@your-platform/movie-booking-sdk",
  "version": "1.0.0-beta.1",
  "description": "Mobile-optimized movie ticket booking SDK for seamless integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/react-native.js",
      "types": "./dist/react-native/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run type-check && npm run lint",
    "prepublishOnly": "npm run build",
    "size": "bundlesize",
    "analyze": "rollup-plugin-analyzer",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "docs:build": "typedoc src/index.ts --out docs",
    "release": "semantic-release",
    "validate": "npm run type-check && npm run lint && npm run test:ci"
  },
  "keywords": [
    "movie",
    "booking",
    "tickets",
    "cinema",
    "sdk",
    "react",
    "react-native",
    "mobile",
    "theater",
    "showtimes",
    "decentralized",
    "media",
    "ecosystem"
  ],
  "author": "Your Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-platform/movie-booking-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-platform/movie-booking-sdk/issues"
  },
  "homepage": "https://github.com/your-platform/movie-booking-sdk#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "bundlesize": "^0.18.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "semantic-release": "^21.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "framer-motion": "^10.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "50kb"
    },
    {
      "path": "./dist/index.esm.js",
      "maxSize": "50kb"
    },
    {
      "path": "./dist/index.umd.js",
      "maxSize": "60kb"
    }
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}